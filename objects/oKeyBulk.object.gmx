}*/

if !((sign(count)==-1 || sign(icount)==-1) &amp;&amp; sign(count)&lt;=0 &amp;&amp; sign(icount)&lt;=0){
    if color != key_MASTER{
        if type == 5 || type == 6 || type == 7{
            scrPlaySoundExt(sndSignflip,1,1,false);
