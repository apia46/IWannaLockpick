}else{
    touched = 1;
    whiteFlash = 1;
}

if type == 0{//Normal
    if !isStar(color,colorCopy){
        addComplexKeys(color,colorCopy,count,icount,0);
    }
}
if type == 1{//Absolute
    if color != key_COPY{
        if !isStar(color,colorCopy){
            global.key[color] = count;
            global.ikey[color] = icount;
        }
    }else{
        if !isStar(color,colorCopy){
            global.key[colorCopy] = count;
            global.ikey[colorCopy] = icount;
        }
    }
}
/*if type == 2{//Scrapped ordinal keys
    if !isStar(color,colorCopy){
        addComplexKeys(color,colorCopy,count,0);
    }
}*/
//Normally I'd shift the other numbers back like in KTool, right?
//But I don't wanna go back and change a bunch of key objects' create events...
if type == 3{//Star Key
    if color != key_COPY{
        global.star[color] = 1;
    }else{
        global.star[colorCopy] = 1;
    }
}
if type == 4{//Unstar Key
    if color != key_COPY{
        global.star[color] = 0;
    }else{
        global.star[colorCopy] = 0;
    }
}
if type == 5{//Signflip
    if !isStar(color,colorCopy){
        if color != key_COPY{
            global.key[color] *= -1;
            global.ikey[color] *= -1;
        }else{
            global.key[colorCopy] *= -1;
            global.ikey[colorCopy] *= -1;
        }
    }
}
var tmp,tmp2;
if type == 6{//Multiply by i
    if !isStar(color,colorCopy){
        if color != key_COPY{
            tmp = global.key[color];
            tmp2 = global.ikey[color];
            global.key[color] = tmp2*-1;
            global.ikey[color] = tmp;
        }else{
            tmp = global.key[colorCopy];
            tmp2 = global.ikey[colorCopy];
            global.key[colorCopy] = tmp2*-1;
            global.ikey[colorCopy] = tmp;
        }
    }
}
if type == 7{//Multiply by -i
    if !isStar(color,colorCopy){
        if color != key_COPY{
            tmp = global.key[color];
            tmp2 = global.ikey[color];
            global.key[color] = tmp2;
            global.ikey[color] = tmp*-1;
        }else{
            tmp = global.key[colorCopy];
            tmp2 = global.ikey[colorCopy];
            global.key[colorCopy] = tmp2;
            global.ikey[colorCopy] = tmp*-1;
        }
    }
}
/*if type == 6{//Positive
    if !isStar(color,colorCopy){
        if color != key_COPY{
            global.key[color] = abs(global.key[color]);
            global.ikey[color] = abs(global.ikey[color]);
        }else{
            global.key[colorCopy] = abs(global.key[colorCopy]);
            global.ikey[colorCopy] = abs(global.ikey[colorCopy]);
