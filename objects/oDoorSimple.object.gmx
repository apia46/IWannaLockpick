        if ((w == 2) &amp;&amp; (h == 2))
            draw_sprite_ext(sprSol2x2, 1, x, y, 1, 1, 0, make_color_rgb(255, 255, 255), 1)
        draw_set_blend_mode(0)
    }
    if aura[0]
    {
        if ((w == 1) &amp;&amp; (h == 1))
            draw_sprite(sprIce, 0, x, y)
        if ((w == 1) &amp;&amp; (h == 2))
            draw_sprite(sprIce1x2, 0, x, y)
        if ((w == 2) &amp;&amp; (h == 2))
            draw_sprite(sprIce2x2, 0, x, y)
        if ((w == 3) &amp;&amp; (h == 2))
